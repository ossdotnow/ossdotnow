{
  "name": "ossdotnow",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev",
    "start": "dotenv -- turbo run start",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v",
    "db:generate": "turbo run db:generate",
    "db:migrate": "dotenv -- turbo run db:migrate",
    "db:push": "dotenv -- turbo run db:push",
    "db:studio": "dotenv -- turbo run db:studio",
    "db:seed": "dotenv -- turbo run db:seed",
    "db:seed-launches": "dotenv -- turbo run db:seed-launches",
    "db:make-all-admin": "dotenv -- turbo run db:make-all-admin"
  },
  "dependencies": {
    "@octokit/core": "^7.0.3",
    "@octokit/request": "^10.0.3",
    "@octokit/rest": "^22.0.0",
    "minimatch": "^10.0.3"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "dotenv-cli": "^10.0.0",
    "eslint": "catalog:",
    "prettier": "^3.6.2",
    "prettier-plugin-sort-imports": "^1.8.8",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "turbo": "^2.5.6",
    "typescript": "catalog:"
  },
  "engines": {
    "bun": ">=1.2.19"
  },
  "packageManager": "bun@1.2.19",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "@hookform/resolvers": "5.1.1",
      "@octokit/plugin-rest-endpoint-methods": "16.0.0",
      "@trpc/client": "11.4.3",
      "@trpc/server": "11.4.3",
      "@trpc/tanstack-react-query": "11.4.3",
      "@types/node": "24",
      "@types/react": "19.1.8",
      "@types/react-dom": "19.1.6",
      "better-auth": "1.2.10",
      "drizzle-orm": "0.44.2",
      "drizzle-zod": "0.8.2",
      "eslint": "9.29.0",
      "lucide-react": "0.518.0",
      "next": "15.4.5",
      "next-themes": "0.4.6",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-hook-form": "7.58.1",
      "server-only": "0.0.1",
      "sonner": "2.0.5",
      "superjson": "2.2.2",
      "typescript": "5.8.3",
      "zod": "3.25.67"
    }
  },
  "trustedDependencies": [
    "@sentry/cli",
    "@tailwindcss/oxide",
    "core-js-pure",
    "esbuild",
    "msgpackr-extract",
    "sharp"
  ]
}
